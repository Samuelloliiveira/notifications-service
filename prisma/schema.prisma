generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Notification {
  id         String    @id
  recipentId String
  content    String
  category   String
  readAt     DateTime?
  createAt   DateTime  @default(now())

  @@index([recipentId])
}
